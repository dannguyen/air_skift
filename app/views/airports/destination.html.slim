h1 From #{{@origin.name}} (#{{@origin.iata}})
h2 to #{{@destination.name}} (#{{@destination.iata}})
	
/ Hardcoded in ATL to LGA for now
/ http://0.0.0.0:3000/airports/374/to/2826

div#route-map[data-origin-lat="#{{@origin.latitude}}" data-origin-long="#{{@origin.longitude}}" data-dest-lat="#{{@destination.latitude}}" data-dest-long="#{{@destination.longitude}}" data-origin-country="#{{@origin.country}}" data-dest-country="#{{@destination.country}}"]

/ table
/   - @routes.each do |route|
/     tr
/       td = route.carrier.name
/       td = route.year
/       td = route.month
/       td = AircraftModel.to_name( route.aircraft_type_id )
/       td = number_with_delimiter route.passengers


- @routes.group_by{|x| x.carrier}.each_pair do |carrier, routes|
	h3 = carrier.name
	table.table.table-striped.table-bordered
		/ - routes.group_by{|r| r.aircraft_type_id }.each_pair do |aircraft_id, x_routes|
		- routes.each do |route|
			tr
				td = Date::MONTHNAMES[route.month]
				td = route.year
				td = AircraftModel.to_name route.aircraft_type_id
				td = number_with_delimiter route.passengers



/ ul
/ - @routes.group_by{|x| x.carrier}.each_pair do |carrier, routes|
/   li
/     => carrier.name
/     ul
/       - routes.each do |route|
/         li => "Aircraft #{route.aircraft_type_id}"
/         li =>  number_with_delimiter route.passengers
/     / => route.year
/     / => route.month
/     / => route.aircraft_type_id
/     / => number_with_delimiter route.passengers
